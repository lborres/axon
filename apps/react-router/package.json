{
  "name": "@axon/client",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "git clean -xdf node_modules",
    "dev": "pnpm with-env vite dev --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test": "vitest run",
    "fmt": "prettier --check . --ignore-path ../../.gitignore --ignore-path .prettierignore",
    "lint": "eslint --flag unstable_native_nodejs_ts_config",
    "typecheck": "tsc --noEmit",
    "ui-add": "pnpm dlx shadcn@latest add && prettier src --write --list-different",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@tailwindcss/postcss": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "@tanstack/react-devtools": "^0.8.1",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-query": "catalog:",
    "@tanstack/react-router-devtools": "^1.136.8",
    "@tanstack/react-router": "^1.136.8",
    "@tanstack/router-plugin": "^1.136.8",
    "postcss": "^8.5.6",
    "react-dom": "catalog:react19",
    "react": "catalog:react19",
    "tailwindcss": "catalog:",
    "tw-animate-css": "^1.4.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@axon/eslint-config": "workspace:*",
    "@axon/prettier-config": "workspace:*",
    "@axon/tailwind-config": "workspace:*",
    "@axon/tsconfig": "workspace:*",
    "@axon/ui": "workspace:*",
    "@tailwindcss/vite": "catalog:",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "catalog:",
    "@types/react-dom": "catalog:react19",
    "@types/react": "catalog:react19",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "catalog:",
    "jsdom": "^27.2.0",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vite-tsconfig-paths": "^5.1.4",
    "vite": "^7.2.2",
    "vitest": "^4.0.9",
    "web-vitals": "^5.1.0"
  }
